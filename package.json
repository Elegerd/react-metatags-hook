    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-terser": "^4.0.4",
    "rollup-plugin-typescript2": "^0.21.0",
    "tslint": "^5.16.0",
    "tslint-config-standard": "^8.0.1",
    "tslint-react": "^4.0.0",
    "tslint-react-hooks": "^2.1.0",
    "typescript": "^3.4.5"
  },
  "peerDependencies": {
    "react": "^16.8.0"
